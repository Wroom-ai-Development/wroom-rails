<div id="<%= dom_id document %>">
  <p>
    <strong>Title:</strong>
    <%= document.title %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= document.notes %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= document.author %>
  </p>

  <p>
    <strong>Year published:</strong>
    <%= document.year_published %>
  </p>

  <p>
    <strong>User:</strong>
    <%= document.user_id %>
  </p>

</div>
