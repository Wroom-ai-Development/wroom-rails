<%= turbo_stream_from 'voices' %>
<div id="voices-manager">
  <%= turbo_frame_tag "voices", src: voices_path %>
  <% if current_user.voices.any? %>
    <%= turbo_frame_tag 'voice_editor', src: edit_voice_path(@voice) %>
  <% else %>
    <%= turbo_frame_tag 'voice_editor', src: new_voice_path %>
  <% end %>
</div>
