<%#= TODO: Rename this turbo frame id to be more descriptive %>
<%= turbo_frame_tag "voices" do %>
  <div class='voices'>
    <div class='table-wrapper'>
      <table>
        <% current_user.voices.kept.order(:name).each do |voice| %>
          <%= render 'voice', voice: voice %>
        <% end %>
        <tr id="new-voices-marker" style="display: none;">
      </table>
    </div>
  </div>
<% end %>
