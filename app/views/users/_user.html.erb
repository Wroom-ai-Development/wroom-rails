<div id="<%= dom_id user %>">

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= "#{user.first_name} #{user.last_name}" %>
  </p>

  <p>
    <strong>ID:</strong>
    <%= user.id %>
  </p>

  <p>
    <strong>Role</strong>
    <%= user.role %>
  </p>
</div>
